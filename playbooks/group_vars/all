db_config:
  mariadb_server:
    dest: /etc/my.cnf.d/server.cnf
    section: mysqld
    option: character-set-server
    value: utf8
  mariadb_client:
    dest: /etc/my.cnf.d/client.cnf
    section: client
    option: default-character-set
    value: utf8
